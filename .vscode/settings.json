{
  "eslint.validate": ["vue", "javascript", "javascriptreact"],
  "editor.formatOnSave": false,
  "vetur.validation.template": false,
  "files.exclude": {
    "**/.lighthouseci/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/coverage/**": true,
    "**/.nuxt/**": true,
    "**/sw.js": true,
  },
  "editor.codeActionsOnSave": {
